{
  "name": "hemant/pagewire",
  "description": "Livewire 4 page builder with Mary UI for Laravel 12.",
  "version": "0.1.0",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Hemant\\Pagewire\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hemant\\Pagewire\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^12.0",
    "livewire/livewire": "^4.0",
    "robsontenorio/mary": "^2.0",
    "spatie/livewire-filepond": "^1.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hemant\\Pagewire\\Providers\\PagewireServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
